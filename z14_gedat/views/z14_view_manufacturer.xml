<odoo>
  <record id="z14_view_manufacturer_tree" model="ir.ui.view">
    <field name="name">z14.gedat.manufacturer.tree</field>
    <field name="model">z14.gedat.manufacturer</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="code"/>
        <field name="is_our_company_code"/>
      </tree>
    </field>
  </record>

  <record id="z14_view_manufacturer_form" model="ir.ui.view">
    <field name="name">z14.gedat.manufacturer.form</field>
    <field name="model">z14.gedat.manufacturer</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="code"/>
            <field name="is_our_company_code"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
