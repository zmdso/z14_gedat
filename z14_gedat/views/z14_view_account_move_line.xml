<odoo>
  <record id="z14_view_move_form_inherit_gedat" model="ir.ui.view">
    <field name="name">account.move.form.z14.gedat</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='invoice_line_ids']/tree" position="inside">
        <field name="z14_gedat_fillings" optional="show"/>
        <field name="z14_gedat_litres" optional="show"/>
        <field name="z14_gedat_hl" optional="show"/>
      </xpath>
    </field>
  </record>
</odoo>
